{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }, {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }, {
            "src": "/api/(.*)/(.*)",
            "dest": "/api/$1/$2"
        }, {
            "src": "/api/(.*)/(.*)/(.*)",
            "dest": "/api/$1/$2/$3"
        }
    ]
}