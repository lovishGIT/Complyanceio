{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1",
            "headers": {
                "Access-Control-Allow-Origin": "https://complyanceio.vercel.app",
                "Access-Control-Allow-Credentials": "true",
                "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST",
                "Access-Control-Allow-Headers": "X-CSRF-Token,X-Requested-With,Accept,Accept-Version,Content-Length,Content-MD5,Content-Type,Date,X-Api-Version,Authorization,Cookie,Set-Cookie,Access-Cookie,Refresh-Token"
            }
        }
    ]
}